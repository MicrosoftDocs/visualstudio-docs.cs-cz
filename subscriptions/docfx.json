{
  "build": {
    "markdownEngineName": "markdig",
    "content": [
      {
        "files": [
          "**/*.md",
          "**/**.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.svg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "feedback_system": "None",
      "feedback_product_url": "https://developercommunity2.visualstudio.com/search?space=8",
      "breadcrumb_path": "/visualstudio/subscriptions/_breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "manager": "lank",
      "ms.topic": "conceptual",
      "ms.prod": "visual-studio-family",
      "ms.technology": "vs-subscriptions",
      "searchScope": [
        "VS Subscriptions"
      ],
      "bilingual_type": "hover over"
    },
    "template": [],
    "dest": "subscriptions",
    "rules": {
      "docs-link-absolute": {
        "exclude": [
          "**/*.md",
          "**/*.yml"
        ]
      }
    },
    "fileMetadata": {}
  }
}